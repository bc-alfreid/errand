<div class="row">
  <div class="col-xs-12 ">
    <div class="panel 0, panel-home-content">
      <div class="panel-body">
        <%= simple_form_for(@profile) do |f| %>
            <%= f.error_notification %>

            <div class="form-inputs">
              <%= f.input :first_name%>
              <%= f.input :last_name %>
              <%= f.input :phone %>
              <%= f.input :adress_line1 %>
              <%= f.input :adress_line2 %>
              <%= f.input :city %>
              <%= f.input :postcode %>
              <%= f.input :state %>
              <%= f.input :country %>
              <%= f.input :phone %>
              <%= f.input :photo %>
            </div>

            <div class="form-actions">
              <%= f.button :submit %>
            </div>
        <% end %>
      </div>
    </div>
  </div>
</div>


